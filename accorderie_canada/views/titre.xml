<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="titre_view_form" model="ir.ui.view">
        <field name="name">titre_form</field>
        <field name="model">titre</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="datemaj_titre" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="notitre" />
                    </group>
                    <group>
                        <field name="titre" />
                    </group>
                    <group>
                        <field name="visible_titre" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="titre_view_tree" model="ir.ui.view">
        <field name="name">titre_tree</field>
        <field name="model">titre</field>
        <field name="arch" type="xml">
            <tree>
                <field name="datemaj_titre" />
                <field name="name" />
                <field name="notitre" />
                <field name="titre" />
                <field name="visible_titre" />
            </tree>
        </field>
    </record>

    <act_window
        id="titre_titre_action_view_action_window"
        name="titre_action_view"
        res_model="titre"
        view_mode="list,form"
    />
</odoo>
