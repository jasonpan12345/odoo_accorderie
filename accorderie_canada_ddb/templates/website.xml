                                                t-if="offre_service.condition_autre"
                                                class="o_default_snippet_text"
                                                t-field="offre_service.condition_autre"
                                            />
                                            <p
                                                t-if="offre_service.date_affichage"
                                                class="o_default_snippet_text"
                                                t-field="offre_service.date_affichage"
                                            />
                                            <p
                                                t-if="offre_service.date_debut"
                                                class="o_default_snippet_text"
                                                t-field="offre_service.date_debut"
