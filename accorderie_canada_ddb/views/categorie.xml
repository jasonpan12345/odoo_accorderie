<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="categorie_view_form" model="ir.ui.view">
        <field name="name">categorie_form</field>
        <field name="model">categorie</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="approuve" />
                    </group>
                    <group>
                        <field name="archive" />
                    </group>
                    <group>
                        <field name="nom" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="categorie_view_tree" model="ir.ui.view">
        <field name="name">categorie_tree</field>
        <field name="model">categorie</field>
        <field name="arch" type="xml">
            <tree>
                <field name="approuve" />
                <field name="archive" />
                <field name="nom" />
            </tree>
        </field>
    </record>

    <act_window id="categorie_categorie_action_window" name="Categorie" res_model="categorie" view_mode="list,form" />
</odoo>
