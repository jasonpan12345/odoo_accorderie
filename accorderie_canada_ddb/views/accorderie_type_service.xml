<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_type_service_view_form" model="ir.ui.view">
        <field name="name">accorderie_type_service_form</field>
        <field name="model">accorderie.type.service</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-archive">
                            <field name="actif" widget="boolean_button" />
                        </button>
                    </div>
                    <group>
                        <field name="nom_complet" />
                    </group>
                    <group>
                        <field name="approuve" />
                    </group>
                    <group>
                        <field name="description" />
                    </group>
                    <group>
                        <field name="identifiant" />
                    </group>
                    <group>
                        <field name="nom" />
                    </group>
                    <group>
                        <field name="numero" />
                    </group>
                    <group>
                        <field name="sous_categorie_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_type_service_view_tree" model="ir.ui.view">
        <field name="name">accorderie_type_service_tree</field>
        <field name="model">accorderie.type.service</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nom_complet" />
                <field name="approuve" />
                <field name="description" />
                <field name="identifiant" />
                <field name="nom" />
                <field name="numero" />
                <field name="sous_categorie_id" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_type_service_accorderie_type_service_action_window"
        name="Accorderie Type Service"
        res_model="accorderie.type.service"
        view_mode="list,form"
    />
</odoo>
