<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="type_compte_view_form" model="ir.ui.view">
        <field name="name">type_compte_form</field>
        <field name="model">type.compte</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="accodeursimple" />
                    </group>
                    <group>
                        <field name="admin" />
                    </group>
                    <group>
                        <field name="adminchef" />
                    </group>
                    <group>
                        <field name="adminordpointservice" />
                    </group>
                    <group>
                        <field name="adminpointservice" />
                    </group>
                    <group>
                        <field name="nom_complet" />
                    </group>
                    <group>
                        <field name="nomembre" />
                    </group>
                    <group>
                        <field name="reseau" />
                    </group>
                    <group>
                        <field name="spip" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="type_compte_view_tree" model="ir.ui.view">
        <field name="name">type_compte_tree</field>
        <field name="model">type.compte</field>
        <field name="arch" type="xml">
            <tree>
                <field name="accodeursimple" />
                <field name="admin" />
                <field name="adminchef" />
                <field name="adminordpointservice" />
                <field name="adminpointservice" />
                <field name="nom_complet" />
                <field name="nomembre" />
                <field name="reseau" />
                <field name="spip" />
            </tree>
        </field>
    </record>

    <act_window
        id="type_compte_type_compte_action_window"
        name="Type Compte"
        res_model="type.compte"
        view_mode="list,form"
    />
</odoo>
