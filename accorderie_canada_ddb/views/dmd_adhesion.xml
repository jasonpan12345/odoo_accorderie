<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="dmd_adhesion_view_form" model="ir.ui.view">
        <field name="name">dmd_adhesion_form</field>
        <field name="model">dmd.adhesion</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="courriel" />
                    </group>
                    <group>
                        <field name="datemaj" />
                    </group>
                    <group>
                        <field name="enattente" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="noaccorderie" />
                    </group>
                    <group>
                        <field name="nodmdadhesion" />
                    </group>
                    <group>
                        <field name="nom" />
                    </group>
                    <group>
                        <field name="poste" />
                    </group>
                    <group>
                        <field name="prenom" />
                    </group>
                    <group>
                        <field name="supprimer" />
                    </group>
                    <group>
                        <field name="telephone" />
                    </group>
                    <group>
                        <field name="transferer" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="dmd_adhesion_view_tree" model="ir.ui.view">
        <field name="name">dmd_adhesion_tree</field>
        <field name="model">dmd.adhesion</field>
        <field name="arch" type="xml">
            <tree>
                <field name="courriel" />
                <field name="datemaj" />
                <field name="enattente" />
                <field name="name" />
                <field name="noaccorderie" />
                <field name="nodmdadhesion" />
                <field name="nom" />
                <field name="poste" />
                <field name="prenom" />
                <field name="supprimer" />
                <field name="telephone" />
                <field name="transferer" />
            </tree>
        </field>
    </record>

    <act_window
        id="dmd_adhesion_dmd_adhesion_action_window"
        name="Dmd Adhesion"
        res_model="dmd.adhesion"
        view_mode="list,form"
    />
</odoo>
