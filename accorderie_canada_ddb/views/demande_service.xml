<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="demande_service_view_form" model="ir.ui.view">
        <field name="name">demande_service_form</field>
        <field name="model">demande.service</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="approuve" />
                    </group>
                    <group>
                        <field name="datedebut" />
                    </group>
                    <group>
                        <field name="datefin" />
                    </group>
                    <group>
                        <field name="description" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="noaccorderie" />
                    </group>
                    <group>
                        <field name="nodemandeservice" />
                    </group>
                    <group>
                        <field name="nomembre" />
                    </group>
                    <group>
                        <field name="supprimer" />
                    </group>
                    <group>
                        <field name="titredemande" />
                    </group>
                    <group>
                        <field name="transmit" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="demande_service_view_tree" model="ir.ui.view">
        <field name="name">demande_service_tree</field>
        <field name="model">demande.service</field>
        <field name="arch" type="xml">
            <tree>
                <field name="approuve" />
                <field name="datedebut" />
                <field name="datefin" />
                <field name="description" />
                <field name="name" />
                <field name="noaccorderie" />
                <field name="nodemandeservice" />
                <field name="nomembre" />
                <field name="supprimer" />
                <field name="titredemande" />
                <field name="transmit" />
            </tree>
        </field>
    </record>

    <act_window
        id="demande_service_demande_service_action_window"
        name="Demande Service"
        res_model="demande.service"
        view_mode="list,form"
    />
</odoo>
