<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_type_tel_view_form" model="ir.ui.view">
        <field name="name">accorderie_type_tel_form</field>
        <field name="model">accorderie.type.tel</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="nom" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_type_tel_view_tree" model="ir.ui.view">
        <field name="name">accorderie_type_tel_tree</field>
        <field name="model">accorderie.type.tel</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nom" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_type_tel_accorderie_type_tel_action_window"
        name="Accorderie Type Tel"
        res_model="accorderie.type.tel"
        view_mode="list,form"
    />
</odoo>
