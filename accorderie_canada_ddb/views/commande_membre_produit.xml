                <field name="sitaxablep_facture" />
            </tree>
        </field>
    </record>

    <act_window
        id="commande_membre_produit_commande_membre_produit_action_window"
        name="Commande Membre Produit"
        res_model="commande.membre.produit"
        view_mode="list,form"
    />
</odoo>
