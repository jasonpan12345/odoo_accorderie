<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="origine_view_form" model="ir.ui.view">
        <field name="name">origine_form</field>
        <field name="model">origine</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="noorigine" />
                    </group>
                    <group>
                        <field name="origine" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="origine_view_tree" model="ir.ui.view">
        <field name="name">origine_tree</field>
        <field name="model">origine</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="noorigine" />
                <field name="origine" />
            </tree>
        </field>
    </record>

    <act_window id="origine_origine_action_window" name="Origine" res_model="origine" view_mode="list,form" />
</odoo>
