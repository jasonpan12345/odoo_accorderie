<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="revenu_familial_view_form" model="ir.ui.view">
        <field name="name">revenu_familial_form</field>
        <field name="model">revenu.familial</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="revenu" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="revenu_familial_view_tree" model="ir.ui.view">
        <field name="name">revenu_familial_tree</field>
        <field name="model">revenu.familial</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="revenu" />
            </tree>
        </field>
    </record>

    <act_window
        id="revenu_familial_revenu_familial_action_window"
        name="Revenu Familial"
        res_model="revenu.familial"
        view_mode="list,form"
    />
</odoo>
