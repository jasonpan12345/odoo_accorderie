<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_revenu_familial_view_form" model="ir.ui.view">
        <field name="name">accorderie_revenu_familial_form</field>
        <field name="model">accorderie.revenu.familial</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="nom" />
                    </group>
                    <group>
                        <field name="membre" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_revenu_familial_view_tree" model="ir.ui.view">
        <field name="name">accorderie_revenu_familial_tree</field>
        <field name="model">accorderie.revenu.familial</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nom" />
                <field name="membre" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_revenu_familial_revenu_familial_action_window"
        name="Revenu familial"
        res_model="accorderie.revenu.familial"
        view_mode="list,form"
    />
</odoo>
