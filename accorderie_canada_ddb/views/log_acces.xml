<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="log_acces_view_form" model="ir.ui.view">
        <field name="name">log_acces_form</field>
        <field name="model">log.acces</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="dateheure_deconnexion" />
                    </group>
                    <group>
                        <field name="dateheureconnexion" />
                    </group>
                    <group>
                        <field name="id_log_acces" />
                    </group>
                    <group>
                        <field name="ip_client_v4" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="navigateur" />
                    </group>
                    <group>
                        <field name="nomembre" />
                    </group>
                    <group>
                        <field name="nomusageressayer" />
                    </group>
                    <group>
                        <field name="referer" />
                    </group>
                    <group>
                        <field name="resolution_h" />
                    </group>
                    <group>
                        <field name="resolution_w" />
                    </group>
                    <group>
                        <field name="statut" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="log_acces_view_tree" model="ir.ui.view">
        <field name="name">log_acces_tree</field>
        <field name="model">log.acces</field>
        <field name="arch" type="xml">
            <tree>
                <field name="dateheure_deconnexion" />
                <field name="dateheureconnexion" />
                <field name="id_log_acces" />
                <field name="ip_client_v4" />
                <field name="name" />
                <field name="navigateur" />
                <field name="nomembre" />
                <field name="nomusageressayer" />
                <field name="referer" />
                <field name="resolution_h" />
                <field name="resolution_w" />
                <field name="statut" />
            </tree>
        </field>
    </record>

    <act_window id="log_acces_log_acces_action_window" name="Log Acces" res_model="log.acces" view_mode="list,form" />
</odoo>
