<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_type_service_sous_categorie_view_form" model="ir.ui.view">
        <field name="name">accorderie_type_service_sous_categorie_form</field>
        <field name="model">accorderie.type.service.sous.categorie</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-archive">
                            <field name="active" widget="boolean_button" />
                        </button>
                    </div>
                    <group>
                        <field name="nom" />
                    </group>
                    <group>
                        <field name="approuver" />
                    </group>
                    <group>
                        <field name="categorie" />
                    </group>
                    <group>
                        <field name="sous_categorie_service" />
                    </group>
                    <group>
                        <field name="type_service" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_type_service_sous_categorie_view_tree" model="ir.ui.view">
        <field name="name">accorderie_type_service_sous_categorie_tree</field>
        <field name="model">accorderie.type.service.sous.categorie</field>
        <field name="arch" type="xml">
            <tree>
                <field name="nom" />
                <field name="approuver" />
                <field name="categorie" />
                <field name="sous_categorie_service" />
                <field name="type_service" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_type_service_sous_categorie_accorderie_type_service_sous_categorie_action_window"
        name="Accorderie Type Service Sous Categorie"
        res_model="accorderie.type.service.sous.categorie"
        view_mode="list,form"
    />
</odoo>
