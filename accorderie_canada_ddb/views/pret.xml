<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="pret_view_form" model="ir.ui.view">
        <field name="name">pret_form</field>
        <field name="model">pret</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="datecomitepret" />
                    </group>
                    <group>
                        <field name="datedemandepret" />
                    </group>
                    <group>
                        <field name="datemaj_pret" />
                    </group>
                    <group>
                        <field name="datepret" />
                    </group>
                    <group>
                        <field name="id_pret" />
                    </group>
                    <group>
                        <field name="montantaccorder" />
                    </group>
                    <group>
                        <field name="montantdemande" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="nbremois" />
                    </group>
                    <group>
                        <field name="nbrepaiement" />
                    </group>
                    <group>
                        <field name="nomembre" />
                    </group>
                    <group>
                        <field name="nomembre_intermediaire" />
                    </group>
                    <group>
                        <field name="nomembre_responsable" />
                    </group>
                    <group>
                        <field name="note" />
                    </group>
                    <group>
                        <field name="raisonemprunt" />
                    </group>
                    <group>
                        <field name="recommandation" />
                    </group>
                    <group>
                        <field name="si_pretaccorder" />
                    </group>
                    <group>
                        <field name="tautinteretannuel" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="pret_view_tree" model="ir.ui.view">
        <field name="name">pret_tree</field>
        <field name="model">pret</field>
        <field name="arch" type="xml">
            <tree>
                <field name="datecomitepret" />
                <field name="datedemandepret" />
                <field name="datemaj_pret" />
                <field name="datepret" />
                <field name="id_pret" />
                <field name="montantaccorder" />
                <field name="montantdemande" />
                <field name="name" />
                <field name="nbremois" />
                <field name="nbrepaiement" />
                <field name="nomembre" />
                <field name="nomembre_intermediaire" />
                <field name="nomembre_responsable" />
                <field name="note" />
                <field name="raisonemprunt" />
                <field name="recommandation" />
                <field name="si_pretaccorder" />
                <field name="tautinteretannuel" />
            </tree>
        </field>
    </record>

    <act_window id="pret_pret_action_window" name="Pret" res_model="pret" view_mode="list,form" />
</odoo>
