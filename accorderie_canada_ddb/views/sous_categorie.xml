<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sous_categorie_view_form" model="ir.ui.view">
        <field name="name">sous_categorie_form</field>
        <field name="model">sous.categorie</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="approuver" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="nocategorie" />
                    </group>
                    <group>
                        <field name="nosouscategorie" />
                    </group>
                    <group>
                        <field name="nosouscategorieid" />
                    </group>
                    <group>
                        <field name="supprimer" />
                    </group>
                    <group>
                        <field name="titre" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="sous_categorie_view_tree" model="ir.ui.view">
        <field name="name">sous_categorie_tree</field>
        <field name="model">sous.categorie</field>
        <field name="arch" type="xml">
            <tree>
                <field name="approuver" />
                <field name="name" />
                <field name="nocategorie" />
                <field name="nosouscategorie" />
                <field name="nosouscategorieid" />
                <field name="supprimer" />
                <field name="titre" />
            </tree>
        </field>
    </record>

    <act_window
        id="sous_categorie_sous_categorie_action_window"
        name="Sous Categorie"
        res_model="sous.categorie"
        view_mode="list,form"
    />
</odoo>
