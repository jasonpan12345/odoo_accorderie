<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="info_logiciel_bd_view_form" model="ir.ui.view">
        <field name="name">info_logiciel_bd_form</field>
        <field name="model">info.logiciel.bd</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="datecreation" />
                    </group>
                    <group>
                        <field name="derniereversionlogiciel" />
                    </group>
                    <group>
                        <field name="lienweb" />
                    </group>
                    <group>
                        <field name="majoblig" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="noinfologicielbd" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="info_logiciel_bd_view_tree" model="ir.ui.view">
        <field name="name">info_logiciel_bd_tree</field>
        <field name="model">info.logiciel.bd</field>
        <field name="arch" type="xml">
            <tree>
                <field name="datecreation" />
                <field name="derniereversionlogiciel" />
                <field name="lienweb" />
                <field name="majoblig" />
                <field name="name" />
                <field name="noinfologicielbd" />
            </tree>
        </field>
    </record>

    <act_window
        id="info_logiciel_bd_info_logiciel_bd_action_window"
        name="Info Logiciel Bd"
        res_model="info.logiciel.bd"
        view_mode="list,form"
    />
</odoo>
