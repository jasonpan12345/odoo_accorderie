<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="accorderie_fichier_view_form" model="ir.ui.view">
        <field name="name">accorderie_fichier_form</field>
        <field name="model">accorderie.fichier</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="datemaj_fichier" />
                    </group>
                    <group>
                        <field name="id_typefichier" />
                    </group>
                    <group>
                        <field name="noaccorderie" />
                    </group>
                    <group>
                        <field name="nomfichieroriginal" />
                    </group>
                    <group>
                        <field name="nomfichierstokage" />
                    </group>
                    <group>
                        <field name="si_accorderielocalseulement" />
                    </group>
                    <group>
                        <field name="si_admin" />
                    </group>
                    <group>
                        <field name="si_disponible" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="accorderie_fichier_view_tree" model="ir.ui.view">
        <field name="name">accorderie_fichier_tree</field>
        <field name="model">accorderie.fichier</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="datemaj_fichier" />
                <field name="id_typefichier" />
                <field name="noaccorderie" />
                <field name="nomfichieroriginal" />
                <field name="nomfichierstokage" />
                <field name="si_accorderielocalseulement" />
                <field name="si_admin" />
                <field name="si_disponible" />
            </tree>
        </field>
    </record>

    <act_window
        id="accorderie_fichier_accorderie_fichier_action_window"
        name="Accorderie Fichier"
        res_model="accorderie.fichier"
        view_mode="list,form"
    />
</odoo>
