<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="type_tel_view_form" model="ir.ui.view">
        <field name="name">type_tel_form</field>
        <field name="model">type.tel</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="typetel" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="type_tel_view_tree" model="ir.ui.view">
        <field name="name">type_tel_tree</field>
        <field name="model">type.tel</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="typetel" />
            </tree>
        </field>
    </record>

    <act_window id="type_tel_type_tel_action_window" name="Type Tel" res_model="type.tel" view_mode="list,form" />
</odoo>
