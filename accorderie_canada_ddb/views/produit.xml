<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="produit_view_form" model="ir.ui.view">
        <field name="name">produit_form</field>
        <field name="model">produit</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="datemaj_produit" />
                    </group>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="noaccorderie" />
                    </group>
                    <group>
                        <field name="nomproduit" />
                    </group>
                    <group>
                        <field name="notitre" />
                    </group>
                    <group>
                        <field name="taxablef" />
                    </group>
                    <group>
                        <field name="taxablep" />
                    </group>
                    <group>
                        <field name="visible_produit" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="produit_view_tree" model="ir.ui.view">
        <field name="name">produit_tree</field>
        <field name="model">produit</field>
        <field name="arch" type="xml">
            <tree>
                <field name="datemaj_produit" />
                <field name="name" />
                <field name="noaccorderie" />
                <field name="nomproduit" />
                <field name="notitre" />
                <field name="taxablef" />
                <field name="taxablep" />
                <field name="visible_produit" />
            </tree>
        </field>
    </record>

    <act_window id="produit_produit_action_window" name="Produit" res_model="produit" view_mode="list,form" />
</odoo>
